@model assessment_sinar_tambang.Models.MatKul.AddMatkulModel

@{
}

<h1>Tambah Mata Kuliah</h1>
<form method="post" action="Add">
    <div class="mb-3">
        <label for="" class="form-label">Kode Mata Kuliah</label>
        <input type="text" class="form-control" asp-for="Kode_MK">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Nama Mata Kuliah</label>
        <input type="text" class="form-control" asp-for="Nama_MK">
    </div>
    <div class="mb-3">
        <label for="" class="form-label">Jumlah SKS</label>
        <input type="number" class="form-control" asp-for="SKS">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>